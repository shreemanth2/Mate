{
  "version": 2,
  "framework": "vite",
  "buildCommand": "node ./node_modules/vite/bin/vite.js build",
  "devCommand": "vite",
  "installCommand": "npm ci --include=dev",
  "outputDirectory": "build",
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
